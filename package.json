{
  "name": "aws-emr-runner",
  "version": "1.0.0",
  "description": "Create AWS EMR cluster and run emr steps, e.g. submit Spark application",
  "main": "index.js",
  
  "scripts": {
    "test:unit": "mocha chai \"./test/unit\"",
    "test:integration": "mocha \"./test/integration\"",
    "cover": "npm run cover:unit && npm run cover:report",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:integration": "nyc --silent --no-clean npm run test:integration",
    "cover:report": "nyc report --reporter=text -lcov --report-dir=coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1300.0",
    "bluebird": "^3.7.2",
    "commander": "^10.0.0",
    "dedent": "^0.7.0",
    "fstream": "^1.0.12",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.20",
    "make": "^0.0.0",
    "mustache": "^4.0.1",
    "nanoid": "^4.0.0",
    "package": "^1.0.1",
    "promise-retry": "^2.0.1",
    "underscore": "^1.12.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@semantic-release/github": "^8.0.6",
    "chai": "^4.3.6",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "pkg": "^5.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
